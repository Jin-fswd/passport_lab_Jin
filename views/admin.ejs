<div class="container mt-5">
  <h1 class="mb-4">Admin Page</h1>
  <h2>Welcome, <%= userInfo.name %></h2>
  <div class="mt-4">
      <h3>Current Active Sessions:</h3>
      <% datas.forEach(user => { %>
      <div class="card mt-3">
          <div class="card-body">
              <h5 class="card-title">SessionID: <%= user.sessionkey %></h5>
              <p class="card-text">UserID: <%= user.userid %></p>
              <a href="/dashboard/revoke/<%= user.sessionkey %>" class="btn btn-danger">Revoke Session</a>
          </div>
      </div>
      <% }) %>
  </div>
</div>